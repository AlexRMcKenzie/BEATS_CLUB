<%= simple_form_for [community, post],
    data: {test_target: "form"} do |f| %>
  <%= f.input :title, label: false, placeholder: "You Title..." %>
  <%= f.input :content, label: false, placeholder: "Add your post here..." %>
  <%= f.input :photo, label: false, as: :file, placeholder: "Add your photo here..." %>
  <%= f.submit "Create Post", class:"btn btn-primary" %>
<% end %>
