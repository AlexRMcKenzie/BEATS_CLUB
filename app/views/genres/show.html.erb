<h1><%= @genre.name %></h1>
<h3><%= "#{@genre.name} Communities" %></h3>
<ul>
  <% @genre_communities.each do |community|%>
  <li><%= link_to community.name, community_path(community) %></li>
  <% end %>
</ul>
<h3><%= "Friends of #{@genre.name}" %></h3>
<ul>
  <% @genre_profiles.each do |profile|%>
  <li><%= link_to profile.nickname, profile_path(profile) %></li>
  <% end %>
</ul>
